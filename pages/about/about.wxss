.content{
  padding-top: 10rpx;
  background-color: #ccc;
}

.section{
  background-color: white;
  padding-left: 20rpx;
  padding-right: 20rpx;
  margin-top: 80rpx; 
  

}

.itemwrap{
  width: 100%;
  border-bottom: 1px solid #ccc;
}
.item{
  padding-top: 20rpx; 
  width: 100%;
  display: flex;
  flex-direction: right;
}
.item .left{
  width: 50%;
}

.item .right{
  text-align: right;
  width: 50%;
  color: #ccc;
  height: 80rpx;
  font-size: smaller;
}